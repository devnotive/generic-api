  "main": "index.js",
  "scripts": {
    "start": "npm run migrate:up && node dist/index.js",
    "build": "tsc",
    "dev": "npm run migrate:up && nodemon --exec ts-node ./src/index.ts --mode development",
    "migrate": "db-migrate --migrations-dir ./migrations",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "migrate:reset": "npm run migrate reset",
    "migrate:create": "npm run migrate create",
    "seed:create": "npm run migrate create:seeds",
    "seed:up": "npm run migrate up:seeds",
    "seed:down": "npm run migrate down:seeds",
    "seed:reset": "npm run migrate reset:seeds",
    "db:reset": "npm run seed:reset && npm run migrate:reset && npm run migrate:up && npm run seed:up",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "cross-env PURPLE_VEST_NODE_ENV=test PURPLE_VEST_SECRET=PURPLEVESTSECRET nyc mocha -r ts-node/register src/**/*.test.ts --exclude src/config/**/*.ts --timeout 15000 --exit",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "prepare": "husky install"
  },
